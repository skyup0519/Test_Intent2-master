<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andy.test_intent2">

    <application
        android:name=".MyAPP.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Actavity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- <activity android:name=".MyApp"/> -->
        <activity android:name=".Actavity.Rest_color" />
        <activity android:name=".Actavity.Rest_Message" />
        <activity android:name=".Actavity.Activity1" />
        <activity android:name=".Actavity.Activity2" />
        <activity android:name=".Actavity.Activity3" />
        <activity android:name=".Actavity.question_answer" />
        <activity android:name=".Actavity.Activity4" />

    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest>
